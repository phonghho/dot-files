// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+enter",
        "command": "renameFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "enter",
        "command": "-renameFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "cmd+0",
        "command": "workbench.action.openLastEditorInGroup",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "cmd+enter",
        "command": "explorer.openToSide",
        "when": "explorerViewletVisible && filesExplorerFocus"
    }
]